{
  "name": "ngx-webp-polyfill",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Alexis Bradley-Mills <alexis.bradleymills@gmail.com> (turnstileweb.com.au)",
  "repository": {
    "type": "git",
    "url": "https://github.com/turnstileweb/ngx-webp-polyfill.git"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "engineStrict": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test ngx-webp-polyfill",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "release:version:sync": "ts-node --project scripts/tsconfig.json scripts/sync-library-version.ts",
    "release": "ts-node --project scripts/tsconfig.json scripts/release.ts",
    "prebuild:library": "rimraf dist/ngx-webp-polyfill",
    "build:library": "ng build ngx-webp-polyfill",
    "prepackage:library": "yarn build:library && yarn package:readme && yarn package:changelog",
    "package:library": "cd dist/ngx-webp-polyfill && npm pack",
    "package:publish": "ts-node --project scripts/tsconfig.json scripts/publish.ts",
    "package:readme": "copy .\\README.md .\\dist\\ngx-webp-polyfill",
    "package:changelog": "copy .\\CHANGELOG.md .\\dist\\ngx-webp-polyfill"
  },
  "private": false,
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@angular/animations": "~8.2.7",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "~8.2.7",
    "@angular/compiler": "~8.2.7",
    "@angular/core": "~8.2.7",
    "@angular/forms": "~8.2.7",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "~8.2.7",
    "@angular/platform-browser-dynamic": "~8.2.7",
    "@angular/router": "~8.2.7",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.2.1",
    "ngx-webp-polyfill": "0.0.1",
    "p-queue": "^6.1.1",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "web-animations-js": "^2.3.2",
    "webp-hero": "0.0.0-dev.24",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.5",
    "@angular-devkit/build-ng-packagr": "~0.803.8",
    "@angular/cli": "~8.3.5",
    "@angular/compiler-cli": "~8.2.7",
    "@angular/language-service": "~8.2.7",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "husky": "2.4.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "^9.4.2",
    "ng-packagr": "^5.4.0",
    "prettier": "^1.18.2",
    "protractor": "~5.4.0",
    "rimraf": "^3.0.0",
    "standard-version": "^7.0.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.5.3"
  }
}
